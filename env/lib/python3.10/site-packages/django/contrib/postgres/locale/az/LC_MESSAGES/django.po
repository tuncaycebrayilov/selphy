#: env/lib/python3.10/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "PostgreSQL uzantıları"

#: env/lib/python3.10/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/array.py:17
#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Array-dəki %(nth)s element təsdiqlənə bilmir:"

#: env/lib/python3.10/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "İç-içə array-lərin uzunluğu eyni olmalıdır."

#: env/lib/python3.10/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "String-lərin string/null-lara xəritələnmə cədvəli"

#: env/lib/python3.10/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "“%(key)s” dəyəri string və ya null deyil."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "JSON məlumat yüklənə bilmir."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Giriş JSON lüğət olmalıdır."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "İki düzgün dəyər daxil edin."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Aralığın başlanğıcı bitişindən böyük ola bilməz."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "İki tam rəqəm daxil edin."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "İki rəqəm daxil edin."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "İki düzgün tarix/vaxt daxil edin."

#: env/lib/python3.10/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "İki düzgün tarix daxil edin."

#: env/lib/python3.10/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Siyahıda %(show_value)d element var, ən çox %(limit_value)d ola bilər."
msgstr[1] ""
"Siyahıda %(show_value)d element var, ən çox %(limit_value)d ola bilər."

#: env/lib/python3.10/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Siyahıda %(show_value)d element var, ən az %(limit_value)d ola bilər."
msgstr[1] ""
"Siyahıda %(show_value)d element var, ən az %(limit_value)d ola bilər."

#: env/lib/python3.10/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Bəzi açarlar əksikdir: %(keys)s"

#: env/lib/python3.10/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Bəzi bilinməyən açarlar təchiz edilib: %(keys)s"

